{
  "name": "xml-server",
  "version": "0.0.1",
  "description": "HTTP API",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist",
    "build": "npm run clear && npm run build:ts && npm run build:assets",
    "build:assets": "npm run build:assets:xml && npm run build:assets:json",
    "build:assets:xml": "cd src && copy ./**/*.xml ../dist",
    "build:assets:json": "cd src && copy ./**/*.json ../dist",
    "build:ts": "tsc",
    "serve": "cd dist && node index.js",
    "start": "npm run build && npm run serve",
    "test": "NODE_ENV=test NODE_CONFIG_DIR=dist/config tape 'dist/**/__tests__/**/*.test.js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "boom": "7.3.0",
    "config": "3.3.8",
    "fast-xml-parser": "3.12.16",
    "fastify": "2.1.0",
    "fastify-accepts": "0.5.0",
    "get-port": "4.2.0",
    "html-minifier": "^3.5.21",
    "io-ts": "1.8.3",
    "io-ts-reporters": "0.0.21",
    "node-cache": "4.2.0",
    "pino": "5.11.3"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/config": "0.0.34",
    "@types/html-minifier": "^3.5.3",
    "@types/nock": "9.3.1",
    "@types/node": "11.11.5",
    "@types/pino": "5.8.6",
    "@types/proxyquire": "^1.3.28",
    "@types/tape": "4.2.33",
    "copy": "0.3.2",
    "nock": "10.0.6",
    "proxyquire": "^2.1.0",
    "tape": "4.10.1",
    "typescript": "3.3.4000"
  }
}
