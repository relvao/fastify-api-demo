{
  "name": "xml-server",
  "version": "0.0.1",
  "description": "HTTP API",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist",
    "build": "npm run clear && npm run build:ts && npm run build:assets",
    "build:assets": "copy ./src/**/*.xml dist",
    "build:ts": "tsc",
    "serve": "cd dist && node index.js",
    "start": "npm run build && npm run serve",
    "test": "tape 'dist/**/__tests__/**/*.test.js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chaff/fui": "0.0.4",
    "axios": "0.18.0",
    "boom": "7.3.0",
    "config": "3.0.1",
    "fast-xml-parser": "3.12.16",
    "fastify": "2.1.0",
    "fastify-accepts": "0.5.0",
    "get-port": "4.2.0"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/config": "0.0.34",
    "@types/nock": "9.3.1",
    "@types/node": "11.11.5",
    "@types/tape": "4.2.33",
    "copy": "0.3.2",
    "nock": "10.0.6",
    "tape": "4.10.1",
    "typescript": "3.3.4000"
  }
}
